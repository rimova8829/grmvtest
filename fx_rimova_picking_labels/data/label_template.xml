<odoo>

    <template id="qa_picking_label_template">
        <t t-call="web.html_container">
          <t t-set="data_report_margin_top" t-value="8"/>
          <t t-set="data_report_header_spacing" t-value="1"/>
            <t t-foreach="lines" t-as="line">
              <div class="page">
                    <table style="width: 102mm; height: 152mm; text-align:left; border: 1px solid black; padding-left: 0px">
                        <tr>
                            <td>
                              <div class="row">
                                <span>P/N:</span>
                                <span t-esc="line['pn']"/>
                              </div>
                            </td>
                        </tr>
                        <tr>
                            <td>
                              <div class="row">
                                <span>PO:</span>
                                <span t-esc="line['mo']"/>
                              </div>
                            </td>
                        </tr>
                        <tr>
                            <td>
                              <div class="row">
                                <span>Cantidad:</span>
                                <span t-esc="line['qty']"/>
                              </div>
                            </td>
                        </tr>
                        <tr>
                            <td>
                              <div class="row">
                                <span>Lote:</span>
                                <span t-esc="line['lot']"/>
                              </div>
                            </td>
                        </tr>
                        <tr>
                            <td>
                              <div class="row">
                                <span>Liberado por:</span>
                                <span t-esc="line['qa']"/>
                              </div>
                            </td>
                        </tr>
                        <tr>
                            <td>
                              <div class="row">
                                <span>Ubicaci√≥n:</span>
                                <span t-esc="line['location_dest']"/>
                              </div>
                            </td>
                        </tr>
                        <tr>
                            <td>
                              <div class="row">
                                <span>Conteo:</span>
                                <span>1 de 1</span>
                              </div>
                            </td>
                        </tr>
                    </table>
                    <p style="page-break-before:always;"> </p>
                </div>
            </t>    
        </t>
    </template>

    <report id="qa_stock_label"
        model="stock.picking"
        string="Etiqueta de calidad"
        report_type="qweb-pdf"
        name="fx_qa_picking_labels.qa_picking_label_template"
        file="fx_qa_picking_labels.qa_picking_label_template"/>

</odoo>